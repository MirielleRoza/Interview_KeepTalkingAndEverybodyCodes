@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}


<body>
    <h1>Security cameras Utrecht</h1>
    <div id="mapid"></div>
    <div id="source">
        source:
        <a href="https://data.overheid.nl/data/dataset/camera-s">https://data.overheid.nl/data/dataset/camera-s</a>
    </div>
    <main>
        <table id="cameraTableContainer">
            <tbody>
                <tr>
                    <td>
                        <table id="column3">
                            <thead>
                                <tr>
                                    <th colspan="4">Cameras 3</th>
                                </tr>
                                <tr>
                                    <th>Number</th>
                                    <th>Name</th>
                                    <th>Latitude</th>
                                    <th>Longitude</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var entry in Model.divByThree){
                                    <tr>
                                        <td>@entry.CameraNumber</td>
                                        <td>@entry.CameraName</td>
                                        <td>@entry.Latitude</td>
                                        <td>@entry.Longitutude</td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                    </td>
                    <td>
                        <table id="column5">
                            <thead>
                                <tr>
                                    <th colspan="4">Cameras 5</th>
                                </tr>
                                <tr>
                                    <th>Number</th>
                                    <th>Name</th>
                                    <th>Latitude</th>
                                    <th>Longitude</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var entry in Model.divByFive)
                                {
                                    <tr>
                                        <td>@entry.CameraNumber</td>
                                        <td>@entry.CameraName</td>
                                        <td>@entry.Latitude</td>
                                        <td>@entry.Longitutude</td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                    </td>
                    <td>
                        <table id="column15">
                            <thead>
                                <tr>
                                    <th colspan="4">Cameras 3 &amp; 5</th>
                                </tr>
                                <tr>
                                    <th>Number</th>
                                    <th>Name</th>
                                    <th>Latitude</th>
                                    <th>Longitude</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var entry in Model.divByBoth)
                                {
                                    <tr>
                                        <td>@entry.CameraNumber</td>
                                        <td>@entry.CameraName</td>
                                        <td>@entry.Latitude</td>
                                        <td>@entry.Longitutude</td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                    </td>
                    <td>
                        <table id="columnOther">
                            <thead>
                                <tr>
                                    <th colspan="4">Cameras Overig</th>
                                </tr>
                                <tr>
                                    <th>Number</th>
                                    <th>Name</th>
                                    <th>Latitude</th>
                                    <th>Longitude</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var entry in Model.divByNone)
                                {
                                    <tr>
                                        <td>@entry.CameraNumber</td>
                                        <td>@entry.CameraName</td>
                                        <td>@entry.Latitude</td>
                                        <td>@entry.Longitutude</td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                    </td>
                </tr>
            </tbody>
        </table>
    </main>
</body>